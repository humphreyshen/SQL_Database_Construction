SELECT 
id,
zh_name,
region_name,
city_name,
zip_code_districtname,
address
FROM(SELECT 
	P.id,
	P.zh_name,
	P.zip_code,
	P.region,
	P.county,
	P.district,
	P.address,
	P.bye,
  R.region_code,
	R.region_name,
	C.city_id,
	C.city_name,
	Z.zip_code_countyname,
	Z.zip_code_districtname
	FROM dDt_alumnidb_personal AS P
	INNER JOIN dDt_alumnidb_region AS R
	ON P.region=R.region_code
	INNER JOIN dDt_alumnidb_city AS C
	ON P.county=C.city_id
	INNER JOIN dDt_alumnidb_zipcode AS Z
	ON P.zip_code=Z.zip_code
	WHERE ((P.bye=1)AND
		  ((P.zip_code = '205')))
     )AS A
;
