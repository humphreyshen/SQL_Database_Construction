SELECT 
id,
zh_name,
region_name,
city_name
zip_code_countyname,
zip_code_districtname,
address
FROM(SELECT 
	dDt_alumnidb_personal.id,
	dDt_alumnidb_personal.zh_name,
	dDt_alumnidb_personal.zip_code,
	dDt_alumnidb_personal.region,
	dDt_alumnidb_personal.county,
	dDt_alumnidb_personal.district,
	dDt_alumnidb_personal.address,
	dDt_alumnidb_personal.bye,
  dDt_alumnidb_region.region_code,
	dDt_alumnidb_region.region_name,
	dDt_alumnidb_city.city_code,
	dDt_alumnidb_city.city_name,
	dDt_alumnidb_zipcode.zip_code_countyname,
	dDt_alumnidb_zipcode.zip_code_districtname
	FROM dDt_alumnidb_personal
	INNER JOIN dDt_alumnidb_region
	ON dDt_alumnidb_personal.region=dDt_alumnidb_region.region_code
	INNER JOIN dDt_alumnidb_city
	ON dDt_alumnidb_personal.county=dDt_alumnidb_city.city_id
	INNER JOIN dDt_alumnidb_zipcode
	ON dDt_alumnidb_personal.zip_code=dDt_alumnidb_zipcode.zip_code
	WHERE ((dDt_alumnidb_personal.bye=1)AND
		 	  ((dDt_alumnidb_personal.county = 'A')))
     )AS A

;
